                                                  Click="miSearchCoil_Click"
                                                  Visibility="Collapsed">
                                            <MenuItem.Icon>
                                                <cmps:AutoGrayImage SnapsToDevicePixels="True"
                                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                                    Source="pack://application:,,,/YuLinTu.Resources;component/Images/16/Refresh.png">
                                                </cmps:AutoGrayImage>
                                            </MenuItem.Icon>
