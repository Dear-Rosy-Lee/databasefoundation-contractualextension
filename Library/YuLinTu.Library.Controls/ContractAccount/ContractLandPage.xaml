<am:InfoPageBase
    x:Class="YuLinTu.Library.Controls.ContractLandPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:am="clr-namespace:YuLinTu.Library.Controls"
    xmlns:cmps="clr-namespace:YuLinTu.Windows.Wpf.Metro.Components;assembly=YuLinTu.Windows.Wpf.Metro"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:metro="clr-namespace:YuLinTu.Windows.Wpf.Metro;assembly=YuLinTu.Windows.Wpf.Metro"
    xmlns:wpftoolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
    Width="720"
    MinHeight="500"
    Header="地块信息"
    Icon="pack://application:,,,/YuLinTu.Resources;component/Images/16/property.png">
    <am:InfoPageBase.Resources>
        <am:ContractLandConvert x:Key="ContractLandConvert" />
    </am:InfoPageBase.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.Resources>
            <Style x:Key="LableStyle" TargetType="Label">
                <Setter Property="Height" Value="25" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="HorizontalContentAlignment" Value="Left" />
                <Setter Property="HorizontalAlignment" Value="Left" />
            </Style>
        </Grid.Resources>
        <ScrollViewer
            Name="scrollViewer"
            HorizontalScrollBarVisibility="Hidden"
            PanningMode="VerticalOnly"
            VerticalScrollBarVisibility="Auto">
            <Grid>
                <cmps:LoadingIcon
                    Width="16"
                    Height="16"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                    Visibility="{Binding Path=IsBusy, Converter={StaticResource BoolToVisibilityConverter}}" />
                <TabControl
                    Height="600"
                    Margin="10,10,10,0"
                    Visibility="{Binding Path=IsBusy, Converter={StaticResource NotBoolToVisibilityConverter}}">
                    <TabItem Name="tiFoundationInfo" Header="基本信息">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="250" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="260" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>

                            <Label Content="行政地域:" Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                x:Name="txtZoneName"
                                Grid.Column="1"
                                Grid.ColumnSpan="1"
                                Height="30"
                                VerticalContentAlignment="Center"
                                IsReadOnly="True"
                                TabIndex="1" />

                            <Label
                                Grid.Column="3"
                                Content="所有权性质:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbOwnerRight"
                                Grid.Row="0"
                                Grid.Column="4"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="2" />

                            <Label
                                Grid.Row="1"
                                Grid.Column="0"
                                Content="地块名称:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                x:Name="txtName"
                                Grid.Row="1"
                                Grid.Column="1"
                                Grid.ColumnSpan="1"
                                Height="30"
                                VerticalContentAlignment="Center"
                                MaxLength="50"
                                TabIndex="3"
                                Text="{Binding CurrentLand.Name, Mode=TwoWay}" />

                            <cmps:ImageTextItem
                                Grid.Row="1"
                                Grid.Column="3"
                                Margin="5,0,0,0"
                                IsRequired="True"
                                Text="地块编码:" />
                            <DockPanel
                                Grid.Row="1"
                                Grid.Column="4"
                                Grid.ColumnSpan="1"
                                LastChildFill="True">
                                <cmps:MetroTextBox
                                    x:Name="txtCode"
                                    Height="30"
                                    VerticalContentAlignment="Center"
                                    MaxLength="19"
                                    TabIndex="4"
                                    Text="{Binding CurrentLand.LandNumber, Mode=TwoWay}">
                                    <cmps:MetroTextBox.ContentRight>
                                        <metro:ImageButton
                                            BorderBrushHover="{DynamicResource Metro_Window_Style_Foreground_Highlight}"
                                            BorderBrushPressed="{DynamicResource Metro_Window_Style_Foreground_Highlight}"
                                            BorderThickness="1,0,0,0"
                                            Click="mbtnGetCode_Click"
                                            Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Falsh.png"
                                            ToolTip="生成可用地块编码" />
                                    </cmps:MetroTextBox.ContentRight>
                                </cmps:MetroTextBox>
                            </DockPanel>

                            <Label
                                Grid.Row="2"
                                Content="承包方名称:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbVirtualPersonName"
                                Grid.Row="2"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                SelectionChanged="cmbVirtualPersonName_SelectionChanged"
                                TabIndex="5" />

                            <Label
                                Grid.Row="2"
                                Grid.Column="3"
                                Content="共有人名称:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                x:Name="txtOwnerName"
                                Grid.Row="2"
                                Grid.Column="5"
                                Height="30"
                                VerticalContentAlignment="Center"
                                IsReadOnly="True"
                                TabIndex="6" />

                            <Label
                                Grid.Row="3"
                                Grid.Column="0"
                                Content="土地利用类型:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbLandName"
                                Grid.Row="3"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                SelectionChanged="cmbLandName_SelectionChanged"
                                TabIndex="7" />

                            <CheckBox
                                x:Name="cbLandNameSecond"
                                Grid.Row="3"
                                Grid.Column="3"
                                Margin="5,0,0,0"
                                VerticalAlignment="Center"
                                Click="cbLandNameSecond_Click"
                                Content="二级地类:" />
                            <cmps:MetroComboBox
                                x:Name="cmbLandNameSecond"
                                Grid.Row="3"
                                Grid.Column="4"
                                Height="30"
                                Padding="7,0,0,0"
                                IsEnabled="{Binding ElementName=cbLandNameSecond, Path=IsChecked}"
                                SelectionChanged="cmbLandNameSecond_SelectionChanged"
                                TabIndex="8" />

                            <CheckBox
                                x:Name="cbLandNameAlise"
                                Grid.Row="4"
                                Grid.Column="3"
                                Margin="5,0,0,0"
                                VerticalAlignment="Center"
                                Click="cbLandNameAlise_Click"
                                Content="地类名称:" />
                            <cmps:MetroTextBox
                                x:Name="txtLandNameAlise"
                                Grid.Row="4"
                                Grid.Column="4"
                                Height="30"
                                VerticalContentAlignment="Center"
                                IsEnabled="{Binding ElementName=cbLandNameAlise, Path=IsChecked}"
                                MaxLength="25"
                                TabIndex="9" />

                            <Label
                                Grid.Row="4"
                                Grid.Column="0"
                                Content="基本农田:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbIsBase"
                                Grid.Row="4"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="10">
                                <ComboBoxItem Selector.IsSelected="True">是</ComboBoxItem>
                                <ComboBoxItem>否</ComboBoxItem>
                                <ComboBoxItem Height="30" />
                            </cmps:MetroComboBox>

                            <Label
                                Grid.Row="5"
                                Grid.Column="0"
                                Content="实测面积:"
                                Style="{StaticResource LableStyle}" />
                            <DockPanel
                                Grid.Row="5"
                                Grid.Column="1"
                                LastChildFill="True">
                                <Label
                                    Content="(亩)"
                                    DockPanel.Dock="Right"
                                    Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown
                                    x:Name="cmbActualArea"
                                    Height="30"
                                    MaxWidth="490"
                                    VerticalAlignment="Center"
                                    Increment="1"
                                    Maximum="10000000"
                                    Minimum="0"
                                    TabIndex="11"
                                    Value="{Binding CurrentLand.ActualArea, Mode=TwoWay}" />
                            </DockPanel>

                            <Label
                                x:Name="Label1"
                                Grid.Row="5"
                                Grid.Column="3"
                                Content="确权面积:"
                                Style="{StaticResource LableStyle}" />
                            <DockPanel
                                Grid.Row="5"
                                Grid.Column="4"
                                LastChildFill="True">
                                <Label
                                    Content="(亩)"
                                    DockPanel.Dock="Right"
                                    Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown
                                    x:Name="cmbAwareArea"
                                    Height="30"
                                    MaxWidth="490"
                                    VerticalAlignment="Center"
                                    Increment="1"
                                    Maximum="10000000"
                                    Minimum="0"
                                    TabIndex="12"
                                    Value="{Binding CurrentLand.AwareArea, Mode=TwoWay}" />
                            </DockPanel>

                            <Label
                                x:Name="Label2"
                                Grid.Row="6"
                                Grid.Column="0"
                                Content="二轮合同面积:"
                                Style="{StaticResource LableStyle}" />
                            <DockPanel
                                Grid.Row="6"
                                Grid.Column="1"
                                Margin="0,5,0,0"
                                LastChildFill="True">
                                <Label
                                    Content="(亩)"
                                    DockPanel.Dock="Right"
                                    Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown
                                    x:Name="DoubleUpDownTableArea"
                                    Height="30"
                                    MaxWidth="490"
                                    VerticalAlignment="Center"
                                    Increment="1"
                                    LostFocus="DoubleUpDownTableArea_LostFocus"
                                    Maximum="10000000"
                                    Minimum="0"
                                    TabIndex="13"
                                    Value="{Binding Path=CurrentLand.TableArea, UpdateSourceTrigger=PropertyChanged}" />
                            </DockPanel>

                            <Label
                                x:Name="Label3"
                                Grid.Row="6"
                                Grid.Column="3"
                                Content="机动面积:"
                                Style="{StaticResource LableStyle}" />
                            <DockPanel
                                Grid.Row="6"
                                Grid.Column="4"
                                Margin="0,5,0,0"
                                LastChildFill="True">
                                <Label
                                    Content="(亩)"
                                    DockPanel.Dock="Right"
                                    Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown
                                    x:Name="doubleUpDownArea"
                                    Height="30"
                                    MaxWidth="490"
                                    VerticalAlignment="Center"
                                    Increment="1"
                                    Maximum="10000000"
                                    Minimum="0"
                                    TabIndex="14"
                                    Value="{Binding CurrentLand.MotorizeLandArea, Mode=TwoWay}" />
                            </DockPanel>
                            <Label
                                x:Name="Label4"
                                Grid.Row="7"
                                Grid.Column="0"
                                Content="延包面积:"
                                Style="{StaticResource LableStyle}" />
                            <DockPanel
                                Grid.Row="7"
                                Grid.Column="1"
                                Margin="0,5,0,0"
                                LastChildFill="True">
                                <Label
                                    Content="(亩)"
                                    DockPanel.Dock="Right"
                                    Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown
                                    x:Name="doubleContractDelayArea"
                                    Height="30"
                                    MaxWidth="490"
                                    VerticalAlignment="Center"
                                    Increment="1"
                                    Maximum="10000000"
                                    Minimum="0"
                                    TabIndex="15"
                                    Value="{Binding CurrentLand.ContractDelayArea, Mode=TwoWay}" />
                            </DockPanel>
                            <Label
                                Grid.Row="8"
                                Content="地块东至:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="8"
                                Grid.Column="1"
                                Height="30"
                                VerticalContentAlignment="Center"
                                MaxLength="50"
                                TabIndex="16"
                                Text="{Binding CurrentLand.NeighborEast, Mode=TwoWay}" />

                            <Label
                                Grid.Row="8"
                                Grid.Column="3"
                                Content="地块西至:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="8"
                                Grid.Column="4"
                                Height="30"
                                VerticalContentAlignment="Center"
                                MaxLength="50"
                                TabIndex="17"
                                Text="{Binding CurrentLand.NeighborWest, Mode=TwoWay}" />

                            <Label
                                Grid.Row="9"
                                Content="地块南至:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="9"
                                Grid.Column="1"
                                Height="30"
                                VerticalContentAlignment="Center"
                                MaxLength="50"
                                TabIndex="18"
                                Text="{Binding CurrentLand.NeighborSouth, Mode=TwoWay}" />

                            <Label
                                Grid.Row="9"
                                Grid.Column="3"
                                Content="地块北至:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="9"
                                Grid.Column="4"
                                Height="30"
                                VerticalContentAlignment="Center"
                                MaxLength="50"
                                TabIndex="19"
                                Text="{Binding CurrentLand.NeighborNorth, Mode=TwoWay}" />

                            <Label
                                Grid.Row="10"
                                Grid.Column="0"
                                Content="承包方式:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbContractWay"
                                Grid.Row="10"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="20" />

                            <Label
                                Grid.Row="10"
                                Grid.Column="3"
                                Content="地力等级:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbQuliaty"
                                Grid.Row="10"
                                Grid.Column="4"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="21" />

                            <Label
                                Grid.Row="11"
                                Grid.Column="0"
                                Content="土地用途:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbPropous"
                                Grid.Row="11"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="22" />

                            <Label
                                Grid.Row="11"
                                Grid.Column="3"
                                Content="地块类别:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cmbLandType"
                                Grid.Row="11"
                                Grid.Column="4"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="23" />

                            <Label
                                Grid.Row="12"
                                VerticalAlignment="Top"
                                Content="备注:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                x:Name="txtComment"
                                Grid.Row="12"
                                Grid.RowSpan="1"
                                Grid.Column="1"
                                Grid.ColumnSpan="4"
                                Height="60"
                                Margin="0,5,0,0"
                                VerticalContentAlignment="Stretch"
                                AcceptsReturn="True"
                                MaxLength="300"
                                TabIndex="24"
                                Text="{Binding CurrentLand.Comment, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Auto" />

                            <Label
                                Grid.Row="13"
                                VerticalAlignment="Top"
                                Content="修改意见:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                x:Name="txtOpiniont"
                                Grid.Row="13"
                                Grid.RowSpan="1"
                                Grid.Column="1"
                                Grid.ColumnSpan="4"
                                Height="60"
                                Margin="0,5,0,0"
                                VerticalContentAlignment="Stretch"
                                AcceptsReturn="True"
                                MaxLength="300"
                                TabIndex="24"
                                Text="{Binding CurrentLand.Opinion, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Auto" />
                        </Grid>
                    </TabItem>
