<cmps:MetroTextBox x:Class="YuLinTu.Component.CoordinateTransformTask.Controls.FileSelectTestBox"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:metro="clr-namespace:YuLinTu.Windows.Wpf.Metro;assembly=YuLinTu.Windows.Wpf.Metro"
                   xmlns:cmps="clr-namespace:YuLinTu.Windows.Wpf.Metro.Components;assembly=YuLinTu.Windows.Wpf.Metro">
    <cmps:MetroTextBox.ContentRight>
        <metro:ImageButton Click="ImageButton_Click_1"
                           Image="pack://application:,,,/YuLinTu.Resources;component/Images/16/FileOpen.png"
                           BorderBrushHover="{DynamicResource Metro_Window_Style_Foreground_Highlight}"
                           BorderBrushPressed="{DynamicResource Metro_Window_Style_Foreground_Highlight}"
                           BorderThickness="1,0,0,0"></metro:ImageButton>

    </cmps:MetroTextBox.ContentRight>
</cmps:MetroTextBox>
