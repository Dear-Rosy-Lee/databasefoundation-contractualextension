<core:NavigatableWorkpageFrame x:Class="YuLinTu.Component.ContractRegeditBook.ContractRegeditBookFramePage"
                               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                               xmlns:metro="clr-namespace:YuLinTu.Windows.Wpf.Metro;assembly=YuLinTu.Windows.Wpf.Metro"
                               xmlns:cmps="clr-namespace:YuLinTu.Windows.Wpf.Metro.Components;assembly=YuLinTu.Windows.Wpf.Metro"
                               xmlns:core="clr-namespace:YuLinTu.Appwork;assembly=YuLinTu.Appwork.Core"
                               xmlns:tk="http://schemas.xceed.com/wpf/xaml/toolkit"
                               xmlns:viewer="clr-namespace:YuLinTu.Library.Controls;assembly=YuLinTu.Library.Controls"
                               xmlns:resx="clr-namespace:YuLinTu.Library.Resources.Properties;assembly=YuLinTu.Library.Resources"
                               mc:Ignorable="d"
                               d:DesignHeight="480"
                               d:DesignWidth="978"
                               Title="承包权证"
                               Description="承包权证管理"
                               Category="应用"
                               Icon="pack://application:,,,/YuLinTu.Component.ContractRegeditBook;component/Resources/map.png"
                               Image="pack://application:,,,/YuLinTu.Resources;component/Images/Galleries/Apps/Regedit78.png"
                               LeftSidebarLength="265"
                               IsLeftSidebarExpanded="False">
    <core:WorkpageFrame.Toolbar>
        <DockPanel TextElement.FontSize="12" LastChildFill="False" Height="90">
            <cmps:TabControlItemRibbonContainer Text="基本操作"
                                                Name="EditContainer"
                                                x:FieldModifier="public">
                <metro:MetroButton x:Name="mbtnAdd"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="添加"
                                        ToolTip="添加承包经营权证"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Add.png" />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnEdit"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="编辑"
                                        ToolTip="编辑承包经营权证信息"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/编辑数据32.png" />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnDel"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="删除"
                                        ToolTip="删除承包经营权证"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Clear.png" />
                </metro:MetroButton>
            </cmps:TabControlItemRibbonContainer>
            <cmps:Separator />
            <cmps:TabControlItemRibbonContainer Text="登记簿数据处理"
                                                Name="ConcordDataContainer"
                                                x:FieldModifier="public">
                <metro:MetroButton x:Name="mbtnInitalizeData"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="权证登记"
                                        ToolTip="根据当前地域下承包合同信息登记权证信息"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/FileMarkAsFinal.png" />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnPrivewRegeditBook"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="预览登记簿"
                                        ToolTip="预览当前选择登记簿信息"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/MeasureBook.png" />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnExportRegeditBook"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="导出登记簿"
                                        ToolTip="导出当前选择登记簿数据"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/导出word_32.png" />
                </metro:MetroButton>
            </cmps:TabControlItemRibbonContainer>
            <cmps:Separator />
            <cmps:TabControlItemRibbonContainer x:FieldModifier="public" Text="证书数据处理" 
                                                Name="BookDataContainer">
                <metro:MetroButton x:Name="mbtnBookPrintSetting"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="证书设置"
                                        ToolTip="证书设置"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/InitialData.png" />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnPrintViewWarrant"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="预览证书"
                                        ToolTip="预览当前选择证书"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/MeasureBook.png" />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnExportWarrant"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="导出证书"
                                        ToolTip="导出当前选择证书"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/导出word_32.png" />
                </metro:MetroButton>
            </cmps:TabControlItemRibbonContainer>
            <cmps:Separator />
            <cmps:TabControlItemRibbonContainer Text="工具"
                                                Name="contractStyleContainer"
                                                x:FieldModifier="public">
                <tk:DropDownButton  Name="mbtnCollectBook"
                                    x:FieldModifier="public"
                                    Padding="4,2,4,2"
                                    VerticalContentAlignment="Top"
                                    cmps:DropDownButtonAttacher.ArrowMargin="0,0,0,3"
                                    cmps:DropDownButtonAttacher.Orientation="Vertical"
                                    cmps:DropDownButtonAttacher.ArrowHorizontalAlignment="Center"
                                    cmps:DropDownButtonAttacher.ArrowVerticalAlignment="Bottom">
                    <tk:DropDownButton.Content>
                        <cmps:ImageTextItem ImagePosition="Top"
                                            Text="汇总表"
                                            ToolTip="导出当前地域下权证汇总信息表"
                                            Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Excel.png" />
                    </tk:DropDownButton.Content>
                    <tk:DropDownButton.DropDownContent>
                        <StackPanel>
                            <metro:MetroButton x:Name="mbtnWarrantSummery"
                                               Padding="8,4"
                                               VerticalContentAlignment="Stretch"
                                               HorizontalContentAlignment="Left"
                                               x:FieldModifier="public">
                                <cmps:ImageTextItem ImagePosition="Left"
                                                    Text="数据汇总表"
                                                    ToolTip="导出当前地域下权证汇总信息表"
                                                    Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Excel.png" />
                            </metro:MetroButton>
                            <metro:MetroButton x:Name="mbtnFamilyTable"
                                               Padding="8,4"
                                               VerticalContentAlignment="Stretch"
                                               HorizontalContentAlignment="Left"
                                               x:FieldModifier="public">
                                <cmps:ImageTextItem ImagePosition="Left"
                                                    Text="单户确认表"
                                                    ToolTip="导出承包方确认表"
                                                    Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Excel.png" />
                            </metro:MetroButton>
                            <metro:MetroButton x:Name="mbtnExportAwareTable"
                                               Padding="8,4"
                                               VerticalContentAlignment="Stretch"
                                               HorizontalContentAlignment="Left"
                                               x:FieldModifier="public">
                                <cmps:ImageTextItem ImagePosition="Left"
                                                    Text="颁证清册"
                                                    ToolTip="导出当前地域下颁证清册表"
                                                    Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Excel.png" />
                            </metro:MetroButton>
                        </StackPanel>
                    </tk:DropDownButton.DropDownContent>
                </tk:DropDownButton>
                <metro:MetroButton x:Name="mbtnClear"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="清空"
                                        ToolTip="清空选择地域下所有合同数据"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/清空32.png" />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnRefresh"
                                   Padding="4,2,4,2"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="刷新"
                                        ToolTip="刷新"
                                        Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/刷新32.png" />
                </metro:MetroButton>

                <metro:MetroButton x:Name="mbtnResetSerialNumber"
                                   Padding="4,2,4,2" 
                                   VerticalContentAlignment="Top"
                                   x:FieldModifier="public"
                                   Visibility="Visible">
                    <cmps:ImageTextItem ImagePosition="Top"
                                        Text="重置流水号"
                                        ToolTip="重置流水号"
                                        Image="pack://application:,,,/YuLinTu.Component.ContractRegeditBook;component/Resources/ResetSerialNumber.png"  />
                </metro:MetroButton>
                <metro:MetroButton x:Name="mbtnRelationSerialNumber"
                                   Padding="8,4"
                                   VerticalContentAlignment="Stretch"
                                   x:FieldModifier="public"
                                   Visibility="Visible">
                    <cmps:ImageTextItem ImagePosition="Top" 
                                        Text="关联流水号"
                                        ToolTip="通过excel关联流水号,excel包含两列，第一列权证编号，第二列流水号(例： X(XXXX)XXX农村土地承包经营权证第XXXXXX号)"
                                        Margin="0,5,0,10"
                                        Image="pack://application:,,,/YuLinTu.Component.ContractRegeditBook;component/Resources/RelateNumber.png"   />
                </metro:MetroButton>

            </cmps:TabControlItemRibbonContainer>
            <cmps:Separator />


        </DockPanel>
    </core:WorkpageFrame.Toolbar>
    <core:NavigatableWorkpageFrame.LeftToolbar>
        <DockPanel LastChildFill="False">
        </DockPanel>
    </core:NavigatableWorkpageFrame.LeftToolbar>
    <Grid>
        <viewer:ContractRegeditBookPanel x:Name="contractRegeditBookPanel"
                                         x:FieldModifier="public" />
    </Grid>
</core:NavigatableWorkpageFrame>
