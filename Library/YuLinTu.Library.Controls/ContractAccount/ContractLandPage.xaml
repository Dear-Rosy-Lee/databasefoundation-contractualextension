                    <TabItem Header="其他信息"
                             Name="tiOtherInfo">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="250" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="260" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Label Content="图幅编号:"
                                   VerticalAlignment="Center"
                                   Grid.Row="0"
                                   Grid.Column="0" />
                            <cmps:MetroTextBox Text="{Binding LandExpand.ImageNumber,Mode=TwoWay}"
                                               Grid.Row="0"
                                               Height="30"
                                               Grid.Column="1"
                                               Grid.ColumnSpan="1"
                                               TabIndex="1" />

                            <Label Content="指界人:"
                                   VerticalAlignment="Center"
                                   Grid.Row="0"
                                   Grid.Column="3" />
                            <cmps:MetroTextBox Grid.Row="0"
                                               Text="{Binding LandExpand.ReferPerson,Mode=TwoWay}"
                                               Height="30"
                                               Grid.Column="4"
                                               TabIndex="2" />

                            <Label Content="耕保类型:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="1"
                                   Grid.Column="0" />
                            <cmps:MetroComboBox x:Name="cbPLowType"
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                TabIndex="3" />

                            <Label Content="经营方式:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="1"
                                   Grid.Column="3" />
                            <cmps:MetroComboBox  x:Name="cbTanscateType"
                                                 Grid.Row="1"
                                                 Height="30"
                                                 Grid.Column="4"
                                                 Padding="7,0,0,0"
                                                 TabIndex="4" />

                            <Label Content="海拔高度:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="2"
                                   Grid.Column="0" />
                            <wpftoolkit:DoubleUpDown Minimum="-10000"
                                                     Height="30"
                                                     Grid.Column="1"
                                                     Grid.Row="2"
                                                     VerticalAlignment="Center"
                                                     Increment="1"
                                                     Value="{Binding LandExpand.Elevation,Mode=TwoWay}"
                                                     Maximum="10000"
                                                     TabIndex="5" />

                            <Label Content="是否飞地:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="2"
                                   Grid.Column="3" />
                            <cmps:MetroComboBox x:Name="cbIsFly"
                                                Grid.Row="2"
                                                Height="30"
                                                Grid.Column="4"
                                                Padding="7,0,0,0"
                                                TabIndex="6">
                                <ComboBoxItem>是</ComboBoxItem>
                                <ComboBoxItem Selector.IsSelected="True">否</ComboBoxItem>
                            </cmps:MetroComboBox>

                            <Label Content="利用情况:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Column="0"
                                   Grid.Row="3" />
                            <cmps:MetroTextBox Grid.Row="3"
                                               Text="{Binding LandExpand.UseSituation,Mode=TwoWay}"
                                               Grid.Column="1"
                                               Height="30"
                                               Padding="7,0,0,0"
                                               TabIndex="7"
                                               Grid.ColumnSpan="1" />

                            <Label Content="产量情况:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="3"
                                   Grid.Column="3" />
                            <cmps:MetroTextBox Text="{Binding LandExpand.Yield,Mode=TwoWay}"
                                               Grid.Row="3"
                                               Height="30"
                                               Grid.Column="4"
                                               TabIndex="8" />

                            <Label Content="产值情况:"
                                   VerticalAlignment="Center"
                                   Grid.Row="4"
                                   Grid.Column="0" />
                            <cmps:MetroTextBox  Text="{Binding LandExpand.OutputValue,Mode=TwoWay}"
                                                Grid.Row="4"
                                                Height="30"
                                                Grid.Column="1"
                                                TabIndex="9" />

                            <Label Content="收益情况:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="4"
                                   Grid.Column="3" />
                            <cmps:MetroTextBox Text="{Binding LandExpand.IncomeSituation,Mode=TwoWay}"
                                               Grid.Row="4"
                                               Height="30"
                                               Grid.Column="4"
                                               TabIndex="10" />

                            <Label Content="种植类型:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="5"
                                   Grid.Column="0" />
                            <cmps:MetroComboBox x:Name="cbPlandType"
                                                Height="30"
                                                Grid.Row="5"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                TabIndex="11" />

                            <Label Content="宗地畦数:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="5"
                                   Grid.Column="3" />
                            <cmps:MetroTextBox Grid.Row="5"
                                               Text="{Binding CurrentLand.PlotNumber,Mode=TwoWay}"
                                               Height="30"
                                               Grid.Column="4"
                                               TabIndex="12" />

                            <Label Content="是否流转:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="6"
                                   Grid.Column="0" />
                            <cmps:MetroComboBox x:Name="cbIsTransfor"
                                                Grid.Row="6"
                                                Height="30"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                TabIndex="13">
                                <ComboBoxItem>是</ComboBoxItem>
                                <ComboBoxItem Selector.IsSelected="True">否</ComboBoxItem>
                            </cmps:MetroComboBox>

                            <Label Content="流转方式:"
                                   VerticalAlignment="Center"
                                   Grid.Row="6"
                                   Grid.Column="3" />
                            <cmps:MetroComboBox x:Name="cbTranscateWay"
                                                IsEnabled="{Binding ElementName=cbIsTransfor,Path=SelectedIndex,Converter={StaticResource ContractLandConvert}}"
                                                Grid.Row="6"
                                                Height="30"
                                                Grid.Column="4"
                                                Padding="7,0,0,0"
                                                TabIndex="14" />

                            <Label Content="流转面积:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="7"
                                   Grid.Column="0" />
                            <wpftoolkit:DoubleUpDown Minimum="0"
                                                     Height="30"
                                                     Grid.Column="1"
                                                     Grid.Row="7"
                                                     IsEnabled="{Binding ElementName=cbIsTransfor,Path=SelectedIndex,Converter={StaticResource ContractLandConvert}}"
                                                     VerticalAlignment="Center"
                                                     Increment="1"
                                                     Value="{Binding CurrentLand.PertainToArea,Mode=TwoWay}"
                                                     Maximum="1000"
                                                     TabIndex="15" />

                            <Label Content="流转期限:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="7"
                                   Grid.Column="3" />
                            <cmps:MetroTextBox Grid.Row="7"
                                               MaxLength="3"
                                               Text="{Binding CurrentLand.TransferTime,Mode=TwoWay}"
                                               Height="30"
                                               IsEnabled="{Binding ElementName=cbIsTransfor,Path=SelectedIndex,Converter={StaticResource ContractLandConvert}}"
                                               Grid.Column="4"
                                               TabIndex="16" />

                            <Label Content="公示备注:"
                                   Grid.Row="8"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox x:Name="txtPublicComment"
                                               Grid.Row="8"
                                               Grid.Column="1"
                                               Grid.ColumnSpan="4"
                                               Grid.RowSpan="1"
                                               MaxLength="300"
                                               TextWrapping="Wrap"
                                               VerticalScrollBarVisibility="Auto"
                                               AcceptsReturn="True"
                                               VerticalContentAlignment="Stretch"
                                               Height="30"
                                               Text="{Binding LandExpand.PublicityComment,Mode=TwoWay}"
                                               TabIndex="17" />

                            <Label Content="调查员:"
                                   Grid.Row="9"
                                   Grid.Column="0"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox Text="{Binding LandExpand.SurveyPerson,Mode=TwoWay}"
                                               Height="30"
                                               Grid.Row="9"
                                               Grid.Column="1"
                                               TabIndex="18" />

                            <Label Content="调查日期:"
                                   VerticalAlignment="Center"
                                   Grid.Row="9"
                                   Grid.Column="3" />
                            <wpftoolkit:DateTimeUpDown x:Name="dtSurveyData"
                                                       Value="{Binding LandExpand.SurveyDate,Mode=TwoWay}"
                                                       Height="30"
                                                       Grid.Row="9"
                                                       Grid.Column="4"
                                                       Format="ShortDate"
                                                       TabIndex="19" />

                            <Label Content="调查记事:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="10"
                                   Grid.Column="0" />
                            <cmps:MetroTextBox Text="{Binding LandExpand.SurveyChronicle,Mode=TwoWay}"
                                               Height="30"
                                               Grid.Row="10"
                                               Grid.Column="1"
                                               Grid.ColumnSpan="4"
                                               TabIndex="20"
                                               AcceptsReturn="True" 
                                               TextWrapping="Wrap" />

                            <Label Content="审核人:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="11"
                                   Grid.Column="0" />
                            <cmps:MetroTextBox Grid.Row="11"
                                               Text="{Binding LandExpand.CheckPerson,Mode=TwoWay}"
                                               Height="30"
                                               Grid.Column="1"
                                               TabIndex="21" />

                            <Label Content="审核日期:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="11"
                                   Grid.Column="3" />
                            <wpftoolkit:DateTimeUpDown x:Name="dtCheckData"
                                                       Value="{Binding LandExpand.CheckDate,Mode=TwoWay}"
                                                       Height="30"
                                                       Grid.Row="11"
                                                       Grid.Column="4"
                                                       Format="ShortDate"
                                                       TabIndex="22" />

                            <Label Content="审核意见:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Row="12"
                                   Grid.Column="0" />
                            <cmps:MetroTextBox Grid.Row="12"
                                               Text="{Binding LandExpand.CheckOpinion,Mode=TwoWay}"
                                               Height="30"
                                               Grid.Column="1"
                                               Grid.ColumnSpan="4"
                                               TabIndex="23"
                                               AcceptsReturn="True" 
                                               TextWrapping="Wrap" />
                        </Grid>
                    </TabItem>
                    <TabItem Header="界址点信息"
                             x:FieldModifier="public"
                             x:Name="dotItem">
                        <Grid>
                            <Grid.Resources>
                                <am:ContractAccountImgConvert x:Key="ImgConvert" />
                            </Grid.Resources>
                            <cmps:VirtualizingMetroTreeView Grid.Row="0"
                                                            Name="dotView"
                                                            HeaderVisibility="Visible"
                                                            HeaderBorderThickness="0"
                                                            HeaderThumbBorderWidth="0"
                                                            ItemAutoExpand="False"
                                                            ItemPadding="2,4"
                                                            BorderThickness="0"
                                                            BorderBrush="LightGray"
                                                            ExpandItemWhenLeftMouseDoubleClick="False"
                                                            x:FieldModifier="public"
                                                            HeaderFontWeight="Bold"
                                                            MouseDoubleClick="dotView_MouseDoubleClick"
                                                            MouseRightButtonDown="dotView_MouseRightButtonDown"
                                                            Keyboard.PreviewKeyDown="dotView_PreviewKeyDown">
                                <cmps:VirtualizingMetroTreeView.View>
                                    <GridView x:Name="dotGridView"
                                              x:FieldModifier="public">

                                        <GridViewColumn Header="点号"
                                                        Width="100"
                                                        x:Name="dotNumber" >
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <cmps:VirtualizingMetroTreeViewItemToggleButton></cmps:VirtualizingMetroTreeViewItemToggleButton>
                                                        <StackPanel HorizontalAlignment="Left"  Orientation="Horizontal">
                                                            <CheckBox x:Name="cbIsValid"
                                                                      IsChecked="{Binding IsValidUI,Mode=TwoWay}"
                                                                      IsEnabled="{Binding IsEditable,Mode=TwoWay}"
                                                                      VerticalAlignment="Center" />
                                                            <cmps:ImageTextItem Image="{Binding Img,Converter={StaticResource ImgConvert}}"
                                                                                HorizontalAlignment="Left"
                                                                                HorizontalContentAlignment="Left"
                                                                                Margin="2,0,0,0"
                                                                                Text="{Binding DotNumberUI}"
                                                                                VerticalAlignment="Center" />
                                                        </StackPanel>
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="统编号" 
                                                        Width="100"
                                                        x:Name="unityNumber">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock Text="{Binding UnityNumberUI}"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Left"/>
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>

                                        </GridViewColumn>

                                        <GridViewColumn Header="X坐标值"
                                                        Width="135"
                                                        x:Name="xCoordinateValue">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock Text="{Binding XCoordinateUI}"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Left" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn Header="Y坐标值"
                                                        Width="135"
                                                        x:Name="yCoordinateValue">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock Text="{Binding YCoordinateUI}"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Left" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn Header="界标类型"
                                                        Width="120"
                                                        x:Name="dotMarkType">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock Text="{Binding DotMarkType}"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Left" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn Header="界址点类型"
                                                        Width="120"
                                                        x:Name="dotType">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock Text="{Binding DotType}"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Left" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                    </GridView>
                                </cmps:VirtualizingMetroTreeView.View>
                                <cmps:VirtualizingMetroTreeView.ContextMenu>
                                    <ContextMenu FontSize="13"
                                                 Name="dotMenu">
                                        <MenuItem Header="设为起点"
                                                  Name="miStartDot"
                                                  Click="miStartDot_Click">
                                            <MenuItem.Icon>
                                                <Image Source="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/ConstrainNumeric.png"
                                                       RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                       SnapsToDevicePixels="True"
                                                       Stretch="None" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem Header="编辑"
                                                  Name="miEditDot"
                                                  Click="miEditDot_Click">
                                            <MenuItem.Icon>
                                                <Image Source="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/字段16.png"
                                                       RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                       SnapsToDevicePixels="True"
                                                       Stretch="None" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem Header="刷新"
                                                  Name="miRefreshDot"
                                                  Click="miRefreshDot_Click">
                                            <MenuItem.Icon>
                                                <cmps:AutoGrayImage SnapsToDevicePixels="True"
                                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                                    Source="pack://application:,,,/YuLinTu.Resources;component/Images/16/Refresh.png">
                                                </cmps:AutoGrayImage>
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem Header="空间查看"
                                                  Name="miSearchDot"
                                                  Click="miSearchDot_Click"
                                                  Visibility="Collapsed">
                                            <MenuItem.Icon>
                                                <cmps:AutoGrayImage SnapsToDevicePixels="True"
                                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                                    Source="pack://application:,,,/YuLinTu.Resources;component/Images/16/Refresh.png">
                                                </cmps:AutoGrayImage>
                                            </MenuItem.Icon>
                                        </MenuItem>
                                    </ContextMenu>
                                </cmps:VirtualizingMetroTreeView.ContextMenu>
                            </cmps:VirtualizingMetroTreeView>
                        </Grid>
                    </TabItem>
                    <TabItem Header="界址线信息"
                             x:FieldModifier="public"
                             x:Name="coilItem">
                        <Grid>
