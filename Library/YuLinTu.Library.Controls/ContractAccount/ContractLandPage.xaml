                    <TabItem Name="tiOtherInfo" Header="其他信息">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="250" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="260" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Label
                                Grid.Row="0"
                                Grid.Column="0"
                                VerticalAlignment="Center"
                                Content="图幅编号:" />
                            <cmps:MetroTextBox
                                Grid.Row="0"
                                Grid.Column="1"
                                Grid.ColumnSpan="1"
                                Height="30"
                                TabIndex="1"
                                Text="{Binding LandExpand.ImageNumber, Mode=TwoWay}" />

                            <Label
                                Grid.Row="0"
                                Grid.Column="3"
                                VerticalAlignment="Center"
                                Content="指界人:" />
                            <cmps:MetroTextBox
                                Grid.Row="0"
                                Grid.Column="4"
                                Height="30"
                                TabIndex="2"
                                Text="{Binding LandExpand.ReferPerson, Mode=TwoWay}" />

                            <Label
                                Grid.Row="1"
                                Grid.Column="0"
                                Content="耕保类型:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cbPLowType"
                                Grid.Row="1"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="3" />

                            <Label
                                Grid.Row="1"
                                Grid.Column="3"
                                Content="经营方式:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cbTanscateType"
                                Grid.Row="1"
                                Grid.Column="4"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="4" />

                            <Label
                                Grid.Row="2"
                                Grid.Column="0"
                                Content="海拔高度:"
                                Style="{StaticResource LableStyle}" />
                            <wpftoolkit:DoubleUpDown
                                Grid.Row="2"
                                Grid.Column="1"
                                Height="30"
                                VerticalAlignment="Center"
                                Increment="1"
                                Maximum="10000"
                                Minimum="-10000"
                                TabIndex="5"
                                Value="{Binding LandExpand.Elevation, Mode=TwoWay}" />

                            <Label
                                Grid.Row="2"
                                Grid.Column="3"
                                Content="是否飞地:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cbIsFly"
                                Grid.Row="2"
                                Grid.Column="4"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="6">
                                <ComboBoxItem>是</ComboBoxItem>
                                <ComboBoxItem Selector.IsSelected="True">否</ComboBoxItem>
                            </cmps:MetroComboBox>

                            <Label
