                                Grid.RowSpan="1"
                                Grid.Column="1"
                                Grid.ColumnSpan="4"
                                Height="30"
                                VerticalContentAlignment="Stretch"
                                AcceptsReturn="True"
                                MaxLength="300"
                                TabIndex="17"
                                Text="{Binding LandExpand.PublicityComment, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Auto" />

                            <Label
                                Grid.Row="9"
                                Grid.Column="0"
                                Content="调查员:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="9"
                                Grid.Column="1"
                                Height="30"
                                TabIndex="18"
                                Text="{Binding LandExpand.SurveyPerson, Mode=TwoWay}" />

                            <Label
                                Grid.Row="9"
                                Grid.Column="3"
                                VerticalAlignment="Center"
                                Content="调查日期:" />
                            <wpftoolkit:DateTimeUpDown
                                x:Name="dtSurveyData"
                                Grid.Row="9"
                                Grid.Column="4"
                                Height="30"
                                Format="ShortDate"
                                TabIndex="19"
                                Value="{Binding LandExpand.SurveyDate, Mode=TwoWay}" />

                            <Label
                                Grid.Row="10"
                                Grid.Column="0"
                                Content="调查记事:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="10"
                                Grid.Column="1"
                                Grid.ColumnSpan="4"
                                Height="30"
                                AcceptsReturn="True"
                                TabIndex="20"
                                Text="{Binding LandExpand.SurveyChronicle, Mode=TwoWay}"
                                TextWrapping="Wrap" />

                            <Label
                                Grid.Row="11"
                                Grid.Column="0"
                                Content="审核人:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="11"
                                Grid.Column="1"
                                Height="30"
                                TabIndex="21"
                                Text="{Binding LandExpand.CheckPerson, Mode=TwoWay}" />

                            <Label
                                Grid.Row="11"
                                Grid.Column="3"
                                Content="审核日期:"
                                Style="{StaticResource LableStyle}" />
                            <wpftoolkit:DateTimeUpDown
                                x:Name="dtCheckData"
                                Grid.Row="11"
                                Grid.Column="4"
                                Height="30"
                                Format="ShortDate"
                                TabIndex="22"
                                Value="{Binding LandExpand.CheckDate, Mode=TwoWay}" />

                            <Label
                                Grid.Row="12"
                                Grid.Column="0"
                                Content="审核意见:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="12"
                                Grid.Column="1"
                                Grid.ColumnSpan="4"
                                Height="30"
                                AcceptsReturn="True"
                                TabIndex="23"
                                Text="{Binding LandExpand.CheckOpinion, Mode=TwoWay}"
                                TextWrapping="Wrap" />
                        </Grid>
                    </TabItem>
                    <TabItem
                        x:Name="dotItem"
                        x:FieldModifier="public"
                        Header="界址点信息">
                        <Grid>
                            <Grid.Resources>
                                <am:ContractAccountImgConvert x:Key="ImgConvert" />
                            </Grid.Resources>
                            <cmps:VirtualizingMetroTreeView
                                Name="dotView"
                                Grid.Row="0"
                                x:FieldModifier="public"
                                BorderBrush="LightGray"
                                BorderThickness="0"
                                ExpandItemWhenLeftMouseDoubleClick="False"
                                HeaderBorderThickness="0"
                                HeaderFontWeight="Bold"
                                HeaderThumbBorderWidth="0"
                                HeaderVisibility="Visible"
                                ItemAutoExpand="False"
                                ItemPadding="2,4"
                                Keyboard.PreviewKeyDown="dotView_PreviewKeyDown"
                                MouseDoubleClick="dotView_MouseDoubleClick"
                                MouseRightButtonDown="dotView_MouseRightButtonDown">
                                <cmps:VirtualizingMetroTreeView.View>
                                    <GridView x:Name="dotGridView" x:FieldModifier="public">

                                        <GridViewColumn
                                            x:Name="dotNumber"
                                            Width="100"
                                            Header="点号">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <cmps:VirtualizingMetroTreeViewItemToggleButton />
                                                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                                            <CheckBox
                                                                x:Name="cbIsValid"
                                                                VerticalAlignment="Center"
                                                                IsChecked="{Binding IsValidUI, Mode=TwoWay}"
                                                                IsEnabled="{Binding IsEditable, Mode=TwoWay}" />
                                                            <cmps:ImageTextItem
                                                                Margin="2,0,0,0"
                                                                HorizontalAlignment="Left"
                                                                VerticalAlignment="Center"
                                                                HorizontalContentAlignment="Left"
                                                                Image="{Binding Img, Converter={StaticResource ImgConvert}}"
                                                                Text="{Binding DotNumberUI}" />
                                                        </StackPanel>
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn
                                            x:Name="unityNumber"
                                            Width="100"
                                            Header="统编号">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding UnityNumberUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="xCoordinateValue"
                                            Width="135"
                                            Header="X坐标值">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding XCoordinateUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="yCoordinateValue"
                                            Width="135"
                                            Header="Y坐标值">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding YCoordinateUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="dotMarkType"
                                            Width="120"
                                            Header="界标类型">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding DotMarkType}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="dotType"
                                            Width="120"
                                            Header="界址点类型">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding DotType}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </cmps:VirtualizingMetroTreeView.View>
                                <cmps:VirtualizingMetroTreeView.ContextMenu>
                                    <ContextMenu Name="dotMenu" FontSize="13">
                                        <MenuItem
                                            Name="miStartDot"
                                            Click="miStartDot_Click"
                                            Header="设为起点">
                                            <MenuItem.Icon>
                                                <Image
                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                    SnapsToDevicePixels="True"
                                                    Source="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/ConstrainNumeric.png"
                                                    Stretch="None" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem
                                            Name="miEditDot"
                                            Click="miEditDot_Click"
                                            Header="编辑">
                                            <MenuItem.Icon>
                                                <Image
                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                    SnapsToDevicePixels="True"
                                                    Source="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/字段16.png"
                                                    Stretch="None" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem
                                            Name="miRefreshDot"
                                            Click="miRefreshDot_Click"
                                            Header="刷新">
                                            <MenuItem.Icon>
                                                <cmps:AutoGrayImage
                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                    SnapsToDevicePixels="True"
                                                    Source="pack://application:,,,/YuLinTu.Resources;component/Images/16/Refresh.png" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem
                                            Name="miSearchDot"
                                            Click="miSearchDot_Click"
                                            Header="空间查看"
                                            Visibility="Collapsed">
                                            <MenuItem.Icon>
                                                <cmps:AutoGrayImage
                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                    SnapsToDevicePixels="True"
                                                    Source="pack://application:,,,/YuLinTu.Resources;component/Images/16/Refresh.png" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                    </ContextMenu>
                                </cmps:VirtualizingMetroTreeView.ContextMenu>
                            </cmps:VirtualizingMetroTreeView>
                        </Grid>
                    </TabItem>
                    <TabItem
                        x:Name="coilItem"
                        x:FieldModifier="public"
                        Header="界址线信息">
                        <Grid>
                            <Grid.Resources>
                                <am:ContractAccountImgConvert x:Key="ImgConvert" />
                            </Grid.Resources>
                            <cmps:VirtualizingMetroTreeView
                                Name="coilView"
                                Grid.Row="0"
                                x:FieldModifier="public"
                                BorderBrush="LightGray"
                                BorderThickness="0"
                                ExpandItemWhenLeftMouseDoubleClick="False"
                                HeaderBorderThickness="0"
                                HeaderFontWeight="Bold"
                                HeaderThumbBorderWidth="0"
                                HeaderVisibility="Visible"
                                ItemAutoExpand="False"
                                ItemPadding="2,4"
                                MouseDoubleClick="coilView_MouseDoubleClick"
                                MouseLeftButtonDown="coilView_MouseLeftButtonDown"
                                MouseRightButtonDown="coilView_MouseRightButtonDown">
                                <cmps:VirtualizingMetroTreeView.View>
                                    <GridView x:Name="coilGridView" x:FieldModifier="public">

                                        <GridViewColumn
                                            x:Name="coilNumber"
                                            Width="100"
                                            Header="序号">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <cmps:VirtualizingMetroTreeViewItemToggleButton />
                                                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                                            <cmps:ImageTextItem
                                                                Margin="2,0,0,0"
                                                                HorizontalAlignment="Stretch"
                                                                VerticalAlignment="Center"
                                                                HorizontalContentAlignment="Left"
                                                                Image="{Binding Img, Converter={StaticResource ImgConvert}}"
                                                                Text="{Binding CoilNumberUI}" />
                                                        </StackPanel>
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="startPoint"
                                            Width="100"
                                            Header="起点">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding StartPointUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="endPoint"
                                            Width="100"
                                            Header="终点">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding EndPointUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="coilLength"
                                            Width="80"
                                            Header="长度">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding CoilLengthUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="coilProperty"
                                            Width="100"
                                            Header="界址线性质">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding CoilPropertyUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="coilType"
                                            Width="100"
                                            Header="界址线类别">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding CoilTypeUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

                                        <GridViewColumn
                                            x:Name="coilLocation"
                                            Width="100"
                                            Header="界址线位置">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <DockPanel>
                                                        <TextBlock
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding CoilLocatioonUI}" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </cmps:VirtualizingMetroTreeView.View>
                                <cmps:VirtualizingMetroTreeView.ContextMenu>
                                    <ContextMenu Name="coilMenu" FontSize="13">
                                        <MenuItem
                                            Name="miEditCoil"
                                            Click="miEditCoil_Click"
                                            Header="编辑">
                                            <MenuItem.Icon>
                                                <Image
                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                    SnapsToDevicePixels="True"
                                                    Source="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/字段16.png"
                                                    Stretch="None" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem
                                            Name="miRefreshCoil"
                                            Click="miRefreshCoil_Click"
                                            Header="刷新">
                                            <MenuItem.Icon>
                                                <cmps:AutoGrayImage
                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                    SnapsToDevicePixels="True"
                                                    Source="pack://application:,,,/YuLinTu.Resources;component/Images/16/Refresh.png" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                        <MenuItem
                                            Name="miSearchCoil"
                                            Click="miSearchCoil_Click"
                                            Header="空间查看"
                                            Visibility="Collapsed">
                                            <MenuItem.Icon>
                                                <cmps:AutoGrayImage
                                                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                                                    SnapsToDevicePixels="True"
                                                    Source="pack://application:,,,/YuLinTu.Resources;component/Images/16/Refresh.png" />
                                            </MenuItem.Icon>
                                        </MenuItem>
                                    </ContextMenu>
                                </cmps:VirtualizingMetroTreeView.ContextMenu>
                            </cmps:VirtualizingMetroTreeView>
                        </Grid>
                    </TabItem>
                </TabControl>
            </Grid>
        </ScrollViewer>
        <Border
            Grid.Row="1"
            Margin="10,0,10,0"
            BorderBrush="LightGray"
            BorderThickness="0,1,0,0"
            SnapsToDevicePixels="True">
            <DockPanel
                Margin="0,5,0,5"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Bottom"
                DockPanel.Dock="Bottom"
                LastChildFill="False">
                <metro:MetroButton
                    Height="30"
                    Padding="10,0"
                    Command="{x:Static metro:MetroCommands.CloseTabMessageBoxCommand}"
                    DockPanel.Dock="Right"
                    IsEnabled="{Binding CanClose}"
                    Style="{StaticResource Metro_Button_Paneled_Style}"
                    TabIndex="14">
                    <StackPanel Orientation="Horizontal">
                        <cmps:ImageTextItem Image="pack://application:,,,/YuLinTu.Resources;component/Images/16/application--minus.png" Text="取消" />
                    </StackPanel>
                </metro:MetroButton>
                <metro:MetroButton
                    x:Name="mbtnLandOK"
                    Height="30"
                    Margin="0,0,5,0"
                    Padding="10,0"
                    Click="mbtnLandOK_Click"
                    DockPanel.Dock="Right"
                    IsEnabled="{Binding CanClose}"
                    Style="{StaticResource Metro_Button_Paneled_Style}">
                    <cmps:ImageTextItem Image="pack://application:,,,/YuLinTu.Resources;component/Images/16/tick.png" Text="确定" />
                </metro:MetroButton>
                <!--<cmps:SuperButton  DockPanel.Dock="Right"
                                   Padding="10,0"
                                   Height="30"
                                   Margin="0,0,5,0"
                                   BorderBrushDefault="{DynamicResource Metro_Window_Style_BorderBrush_Default}"
                                   BorderThickness="1"
                                   ImagePosition="Left"
                                   Name="spInitialDotCoil"
                                   Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/landGeo.png"
                                   Click="spInitialDotCoil_Click" Visibility="Collapsed" x:FieldModifier="public">
                    <cmps:SuperButton.Content>
                        <TextBlock Text="初始界址" />
                    </cmps:SuperButton.Content>
                </cmps:SuperButton>-->
                <cmps:LoadingIcon
                    Width="16"
                    Height="16"
                    Margin="0,0,5,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    DockPanel.Dock="Right"
                    RenderOptions.BitmapScalingMode="NearestNeighbor"
                    Visibility="{Binding Path=IsConfirming, Converter={StaticResource BoolToVisibilityHiddenConverter}}" />
            </DockPanel>
        </Border>
    </Grid>
</am:InfoPageBase>