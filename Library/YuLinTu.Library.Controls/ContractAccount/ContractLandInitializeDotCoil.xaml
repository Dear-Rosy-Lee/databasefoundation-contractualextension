                                <CheckBox x:Name="cbNeighbor" Content="查找毗邻承包方" Margin="25,0,0,0"
                      VerticalAlignment="Center"  IsChecked="{Binding ElementName=cbVirtualPersonFilter,Path=IsChecked, Mode=OneWay}"/>

                                <CheckBox x:Name="cbIsNeighborExportVillageLevel" Content="查找毗邻承包方到村"  
                      VerticalAlignment="Center" Margin="25,0,0,0"/>
                            </StackPanel>
