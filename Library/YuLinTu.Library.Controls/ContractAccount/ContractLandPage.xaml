                            <cmps:MetroTextBox
                                Grid.Row="4"
                                Grid.Column="4"
                                Height="30"
                                TabIndex="10"
                                Text="{Binding LandExpand.IncomeSituation, Mode=TwoWay}" />

                            <Label
                                Grid.Row="5"
                                Grid.Column="0"
                                Content="种植类型:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cbPlandType"
                                Grid.Row="5"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="11" />

                            <Label
                                Grid.Row="5"
                                Grid.Column="3"
                                Content="宗地畦数:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="5"
                                Grid.Column="4"
                                Height="30"
                                TabIndex="12"
                                Text="{Binding CurrentLand.PlotNumber, Mode=TwoWay}" />

                            <Label
                                Grid.Row="6"
                                Grid.Column="0"
                                Content="是否流转:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox
                                x:Name="cbIsTransfor"
                                Grid.Row="6"
                                Grid.Column="1"
                                Height="30"
                                Padding="7,0,0,0"
                                TabIndex="13">
                                <ComboBoxItem>是</ComboBoxItem>
                                <ComboBoxItem Selector.IsSelected="True">否</ComboBoxItem>
                            </cmps:MetroComboBox>

                            <Label
                                Grid.Row="6"
                                Grid.Column="3"
                                VerticalAlignment="Center"
                                Content="流转方式:" />
