<am:InfoPageBase x:Class="YuLinTu.Library.Controls.ContractLandPage"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:wpftoolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
                 xmlns:metro="clr-namespace:YuLinTu.Windows.Wpf.Metro;assembly=YuLinTu.Windows.Wpf.Metro"
                 xmlns:cmps="clr-namespace:YuLinTu.Windows.Wpf.Metro.Components;assembly=YuLinTu.Windows.Wpf.Metro"
                 xmlns:am="clr-namespace:YuLinTu.Library.Controls"
                 Icon="pack://application:,,,/YuLinTu.Resources;component/Images/16/property.png"
                 Header="地块信息"
                 MinHeight="500"
                 Width="720">
    <am:InfoPageBase.Resources>
        <am:ContractLandConvert x:Key="ContractLandConvert" />
    </am:InfoPageBase.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.Resources>
            <Style x:Key="LableStyle"
                   TargetType="Label">
                <Setter Property="Height"
                        Value="25" />
                <Setter Property="VerticalContentAlignment"
                        Value="Center" />
                <Setter Property="VerticalAlignment"
                        Value="Center" />
                <Setter Property="HorizontalContentAlignment"
                        Value="Left" />
                <Setter Property="HorizontalAlignment"
                        Value="Left" />
            </Style>
        </Grid.Resources>
        <ScrollViewer  HorizontalScrollBarVisibility="Hidden"
                       PanningMode="VerticalOnly"
                       VerticalScrollBarVisibility="Auto"
                       Name="scrollViewer">
            <Grid>
                <cmps:LoadingIcon Width="16"
                                  Height="16"
                                  HorizontalAlignment="Center"
                                  VerticalAlignment="Center"
                                  Visibility="{Binding Path=IsBusy, Converter={StaticResource BoolToVisibilityConverter}}"
                                  RenderOptions.BitmapScalingMode="NearestNeighbor"></cmps:LoadingIcon>
                <TabControl Height="490"
                            Visibility="{Binding Path=IsBusy, Converter={StaticResource NotBoolToVisibilityConverter}}"
                            Margin="10,10,10,0">
                    <TabItem Header="基本信息"
                             Name="tiFoundationInfo">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="250" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="260" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Label Content="行政地域:"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox x:Name="txtZoneName"
                                               Grid.Column="1"
                                               Grid.ColumnSpan="1"
                                               IsReadOnly="True"
                                               Height="30"
                                               VerticalContentAlignment="Center"
                                               TabIndex="1" />

                            <Label Content="所有权性质:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Column="3" />
                            <cmps:MetroComboBox x:Name="cmbOwnerRight"
                                                Grid.Row="0"
                                                Grid.Column="4"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                TabIndex="2" />

                            <Label Content="地块名称:"
                                   Grid.Row="1"
                                   Grid.Column="0"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox x:Name="txtName"
                                               Grid.Row="1"
                                               Grid.Column="1"
                                               Grid.ColumnSpan="1"
                                               Height="30"
                                               VerticalContentAlignment="Center"
                                               Text="{Binding CurrentLand.Name,Mode=TwoWay}"
                                               MaxLength="50"
                                               TabIndex="3" />

                            <cmps:ImageTextItem Text="地块编码:"
                                                Grid.Row="1"
                                                Grid.Column="3"
                                                Margin="5,0,0,0"
                                                IsRequired="True"></cmps:ImageTextItem>
                            <DockPanel Grid.Row="1"
                                       Grid.Column="4"
                                       Grid.ColumnSpan="1"
                                       LastChildFill="True">
                                <cmps:MetroTextBox x:Name="txtCode"
                                                   Height="30"
                                                   VerticalContentAlignment="Center"
                                                   Text="{Binding CurrentLand.LandNumber,Mode=TwoWay}"
                                                   MaxLength="19"
                                                   TabIndex="4">
                                    <cmps:MetroTextBox.ContentRight>
                                        <metro:ImageButton Click="mbtnGetCode_Click"
                                                           Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/Falsh.png"
                                                           BorderBrushHover="{DynamicResource Metro_Window_Style_Foreground_Highlight}"
                                                           BorderBrushPressed="{DynamicResource Metro_Window_Style_Foreground_Highlight}"
                                                           BorderThickness="1,0,0,0"
                                                           ToolTip="生成可用地块编码"></metro:ImageButton>
                                    </cmps:MetroTextBox.ContentRight>
                                </cmps:MetroTextBox>
                            </DockPanel>

                            <Label Content="承包方名称:"
                                   Grid.Row="2"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox x:Name="cmbVirtualPersonName"
                                                Grid.Row="2"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                SelectionChanged="cmbVirtualPersonName_SelectionChanged"
                                                TabIndex="5" />

                            <Label Content="共有人名称:"
                                   Grid.Row="2"
                                   Grid.Column="3"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox x:Name="txtOwnerName"
                                               Grid.Row="2"
                                               Grid.Column="5"
                                               Height="30"
                                               VerticalContentAlignment="Center"
                                               IsReadOnly="True"
                                               TabIndex="6" />

                            <Label Content="土地利用类型:"
                                   Grid.Row="3"
                                   Grid.Column="0"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroComboBox x:Name="cmbLandName"
                                                Grid.Row="3"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                SelectionChanged="cmbLandName_SelectionChanged"
                                                TabIndex="7" />

                            <CheckBox Content="二级地类:"
                                      x:Name="cbLandNameSecond"
                                      Grid.Column="3"
                                      Grid.Row="3"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0"
                                      Click="cbLandNameSecond_Click"
                                      />
                            <cmps:MetroComboBox x:Name="cmbLandNameSecond"
                                                IsEnabled="{Binding ElementName=cbLandNameSecond,Path=IsChecked}"
                                                Grid.Column="4"
                                                Grid.Row="3"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                SelectionChanged="cmbLandNameSecond_SelectionChanged"
                                                TabIndex="8" />

                            <CheckBox Content="地类名称:"
                                      x:Name="cbLandNameAlise"
                                      Grid.Row="4"
                                      Grid.Column="3"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0"
                                      Click="cbLandNameAlise_Click"/>
                            <cmps:MetroTextBox x:Name="txtLandNameAlise"
                                               Grid.Column="4"
                                               Grid.Row="4"
                                               Height="30"
                                               VerticalContentAlignment="Center"
                                               MaxLength="25"
                                               IsEnabled="{Binding ElementName=txtLandNameAlise,Path=IsChecked}"
                                               TabIndex="9" />

                            <Label Content="基本农田:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Column="0"
                                   Grid.Row="4" />
                            <cmps:MetroComboBox x:Name="cmbIsBase"
                                                Grid.Row="4"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                TabIndex="10">
                                <ComboBoxItem Selector.IsSelected="True">是</ComboBoxItem>
                                <ComboBoxItem>否</ComboBoxItem>
                                <ComboBoxItem Height="30"></ComboBoxItem>
                            </cmps:MetroComboBox>

                            <Label Content="实测面积:"
                                   Grid.Column="0"
                                   Grid.Row="5"
                                   Style="{StaticResource LableStyle}" />
                            <DockPanel Grid.Row="5"
                                       Grid.Column="1"
                                       LastChildFill="True">
                                <Label DockPanel.Dock="Right"
                                       Content="(亩)"
                                       Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown x:Name="cmbActualArea"
                                                         Minimum="0"
                                                         MaxWidth="490"
                                                         Height="30"
                                                         Maximum="10000000"
                                                         VerticalAlignment="Center"
                                                         Increment="1"
                                                         Value="{Binding CurrentLand.ActualArea,Mode=TwoWay}"
                                                         TabIndex="11" />
                            </DockPanel>

                            <Label x:Name="Label1" Content="确权面积:"
                                   Grid.Column="3"
                                   Grid.Row="5"
                                   Style="{StaticResource LableStyle}" />
                            <DockPanel Grid.Row="5"
                                       Grid.Column="4"
                                       LastChildFill="True">
                                <Label DockPanel.Dock="Right"
                                       Content="(亩)"
                                       Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown   x:Name="cmbAwareArea"
                                                           Minimum="0"
                                                           MaxWidth="490"
                                                           Height="30"
                                                           Maximum="10000000"
                                                           VerticalAlignment="Center"
                                                           Increment="1"
                                                           Value="{Binding CurrentLand.AwareArea,Mode=TwoWay}"
                                                           TabIndex="12" />
                            </DockPanel>

                            <Label x:Name="Label2" Content="二轮合同面积:"
                                   Grid.Column="0"
                                   Grid.Row="6"
                                   Style="{StaticResource LableStyle}" />
                            <DockPanel Grid.Row="6"
                                       Grid.Column="1"
                                       Margin="0,5,0,0"
                                       LastChildFill="True">
                                <Label  DockPanel.Dock="Right"
                                       Content="(亩)"
                                       Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown 
                                    LostFocus="DoubleUpDownTableArea_LostFocus"
                                                         x:Name="DoubleUpDownTableArea"
                                                          Minimum="0"
                                                          MaxWidth="490"
                                                          Height="30"
                                                          Maximum="10000000"
                                                          VerticalAlignment="Center"
                                                          Increment="1"
                                                          TabIndex="13"
                                                          Value="{Binding Path=CurrentLand.TableArea,UpdateSourceTrigger=PropertyChanged}"/>
                            </DockPanel>

                            <Label x:Name="Label3" Content="机动面积:"
                                   Grid.Column="3"
                                   Grid.Row="6"
                                   Style="{StaticResource LableStyle}" />
                            <DockPanel Grid.Row="6"
                                       Grid.Column="4"
                                       LastChildFill="True"
                                       Margin="0,5,0,0">
                                <Label DockPanel.Dock="Right"
                                       Content="(亩)"
                                       Style="{StaticResource LableStyle}" />
                                <wpftoolkit:DoubleUpDown   x:Name="doubleUpDownArea"
                                                           
                                                            Minimum="0"
                                                            MaxWidth="490"
                                                            Height="30"
                                                            Maximum="10000000"
                                                            VerticalAlignment="Center"
                                                            Increment="1"
                                                            Value="{Binding CurrentLand.MotorizeLandArea,Mode=TwoWay}"
                                                            TabIndex="14" />
                            </DockPanel>

                            <Label Content="地块东至:"
                                   Grid.Row="7"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox Grid.Column="1"
                                               Grid.Row="7"
                                               Height="30"
                                               VerticalContentAlignment="Center"
                                               Text="{Binding CurrentLand.NeighborEast,Mode=TwoWay}"
                                               MaxLength="50"
                                               TabIndex="15" />

                            <Label Content="地块西至:"
                                   Grid.Column="3"
                                   Grid.Row="7"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox  Grid.Column="4"
                                                Grid.Row="7"
                                                Height="30"
                                                VerticalContentAlignment="Center"
                                                Text="{Binding CurrentLand.NeighborWest,Mode=TwoWay}"
                                                MaxLength="50"
                                                TabIndex="16" />

                            <Label Content="地块南至:"
                                   Grid.Row="8"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox  Grid.Column="1"
                                                Grid.Row="8"
                                                Height="30"
                                                VerticalContentAlignment="Center"
                                                Text="{Binding CurrentLand.NeighborSouth,Mode=TwoWay}"
                                                MaxLength="50"
                                                TabIndex="17" />

                            <Label Content="地块北至:"
                                   Grid.Column="3"
                                   Grid.Row="8"
                                   Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox  Grid.Column="4"
                                                Grid.Row="8"
                                                Height="30"
                                                VerticalContentAlignment="Center"
                                                Text="{Binding CurrentLand.NeighborNorth,Mode=TwoWay}"
                                                MaxLength="50"
                                                TabIndex="18" />

                            <Label Content="承包方式:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Column="0"
                                   Grid.Row="9" />
                            <cmps:MetroComboBox x:Name="cmbContractWay"
                                                Grid.Row="9"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                TabIndex="19" />

                            <Label Content="地力等级:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Column="3"
                                   Grid.Row="9" />
                            <cmps:MetroComboBox x:Name="cmbQuliaty"
                                                Grid.Row="9"
                                                Grid.Column="4"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                TabIndex="20" />

                            <Label Content="土地用途:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Column="0"
                                   Grid.Row="10" />
                            <cmps:MetroComboBox x:Name="cmbPropous"
                                                Grid.Row="10"
                                                Grid.Column="1"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                TabIndex="21" />

                            <Label Content="地块类别:"
                                   Style="{StaticResource LableStyle}"
                                   Grid.Column="3"
                                   Grid.Row="10" />
                            <cmps:MetroComboBox x:Name="cmbLandType"
                                                Grid.Row="10"
                                                Grid.Column="4"
                                                Padding="7,0,0,0"
                                                Height="30"
                                                TabIndex="22" />

                            <Label Content="备注:"
                                   Grid.Row="11"
                                   Style="{StaticResource LableStyle}"
                                   VerticalAlignment="Top" />
                            <cmps:MetroTextBox x:Name="txtComment"
                                               Grid.Row="11"
                                               Grid.Column="1"
                                               Grid.ColumnSpan="4"
                                               Grid.RowSpan="2"
                                               MaxLength="300"
                                               TextWrapping="Wrap"
                                               VerticalScrollBarVisibility="Auto"
                                               Margin="0,5,0,0"
                                               AcceptsReturn="True"
                                               VerticalContentAlignment="Stretch"
                                               Height="60"
                                               Text="{Binding CurrentLand.Comment,Mode=TwoWay}"
                                               TabIndex="23" />
                        </Grid>
                    </TabItem>
                    <!--<TabItem Header="四至类型" Name="tiNeghborInfo">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="90"/>
                                <ColumnDefinition Width="250"/>
