                                Grid.Column="4"
                                Height="30"
                                Padding="7,0,0,0"
                                IsEnabled="{Binding ElementName=cbIsTransfor, Path=SelectedIndex, Converter={StaticResource ContractLandConvert}}"
                                TabIndex="14" />

                            <Label
                                Grid.Row="7"
                                Grid.Column="0"
                                Content="流转面积:"
                                Style="{StaticResource LableStyle}" />
                            <wpftoolkit:DoubleUpDown
                                Grid.Row="7"
                                Grid.Column="1"
                                Height="30"
                                VerticalAlignment="Center"
                                Increment="1"
                                IsEnabled="{Binding ElementName=cbIsTransfor, Path=SelectedIndex, Converter={StaticResource ContractLandConvert}}"
                                Maximum="1000"
                                Minimum="0"
                                TabIndex="15"
                                Value="{Binding CurrentLand.PertainToArea, Mode=TwoWay}" />

                            <Label
                                Grid.Row="7"
                                Grid.Column="3"
                                Content="流转期限:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
                                Grid.Row="7"
                                Grid.Column="4"
                                Height="30"
                                IsEnabled="{Binding ElementName=cbIsTransfor, Path=SelectedIndex, Converter={StaticResource ContractLandConvert}}"
                                MaxLength="3"
                                TabIndex="16"
                                Text="{Binding CurrentLand.TransferTime, Mode=TwoWay}" />

                            <Label
                                Grid.Row="8"
                                Content="公示备注:"
                                Style="{StaticResource LableStyle}" />
                            <cmps:MetroTextBox
