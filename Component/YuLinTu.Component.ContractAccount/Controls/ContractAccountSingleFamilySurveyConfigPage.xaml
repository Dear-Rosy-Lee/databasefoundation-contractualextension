<appCore:WorkpageOptionsEditor x:Class="YuLinTu.Component.ContractAccount.ContractAccountSingleFamilySurveyConfigPage"
                               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                               xmlns:wpftoolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
                               xmlns:metro="clr-namespace:YuLinTu.Windows.Wpf.Metro;assembly=YuLinTu.Windows.Wpf.Metro"
                               xmlns:cmps="clr-namespace:YuLinTu.Windows.Wpf.Metro.Components;assembly=YuLinTu.Windows.Wpf.Metro"
                               xmlns:appCore="clr-namespace:YuLinTu.Appwork;assembly=YuLinTu.Appwork.Core"
                               Padding="10,10,10,10">
    <DockPanel LastChildFill="True">
        <cmps:ImageTextItem Text="单户调查表设置"
                            DockPanel.Dock="Top"
                            Image="pack://application:,,,/YuLinTu.Library.Resources;component/Resources/PersonSetting.png"
                            Margin="0,0,0,10">
        </cmps:ImageTextItem>
        <cmps:PropertyGrid x:Name="propertyGrid"
                           IsGroupingEnabled="True"
                           StateColumnVisibility="Visible"></cmps:PropertyGrid>
    </DockPanel>
</appCore:WorkpageOptionsEditor>
