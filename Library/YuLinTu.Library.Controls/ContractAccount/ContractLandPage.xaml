                                                        <TextBlock Text="{Binding StartPointUI}"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Left" />
                                                    </DockPanel>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>

