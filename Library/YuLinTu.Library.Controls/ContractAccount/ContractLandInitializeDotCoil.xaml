                            <ColumnDefinition Width="80"/>
                            <ColumnDefinition Width="180*"/>
                        </Grid.ColumnDefinitions>
                        <Label x:Name="lbAddressPointPrefix" Content="界址标识:" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Style="{StaticResource LableStyle}" />
