                        <!--指定指界人,此控件用于鹿泉需求，默认隐藏-->
                        <CheckBox x:Name="cbLandRefer" Content="指定指界人" Visibility="Collapsed" HorizontalAlignment="Left" IsChecked="false" Grid.Row="0" Grid.Column="2"  Grid.ColumnSpan="2"
                      VerticalAlignment="Center"  Margin="31,0,0,0" x:FieldModifier="public"/>
                        <cmps:MetroTextBox  x:Name="txtLandRefer" Grid.Row="0" IsEnabled="{Binding ElementName=cbLandRefer,Path=IsChecked}" Visibility="Collapsed" Grid.Column="3" Height="30"   Padding="0,0,5,0" x:FieldModifier="public" Margin="70,0,10,0"/>

                    </Grid>

                </Grid>
            </TabItem>

            <TabItem Height="160" x:Name="tbItem2">
                <TabItem.Header >
                    <StackPanel>
                        <CheckBox Name="cbSetProperty" IsChecked="True" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center"/>
                        <TextBlock Text="界址数据赋值" TextWrapping="Wrap" Padding="2"   VerticalAlignment="Center" HorizontalAlignment="Center" Width="20"/>
                    </StackPanel>
                </TabItem.Header>
                <Grid Name="MainGrid2" IsEnabled="{Binding ElementName=cbSetProperty,Path=IsChecked}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
