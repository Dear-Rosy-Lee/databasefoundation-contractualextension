                            <cmps:MetroTextBox  Grid.Column="5" Grid.Row="0" Height="30" VerticalContentAlignment="Center"
                                   Text="{Binding CurrentLand.NeighborWest,Mode=TwoWay}" MaxLength="50" TabIndex="16"/>

